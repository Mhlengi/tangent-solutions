[run]
source=
    tangent_solutions
    core

[html]
directory = htmlcov

[report]
exclude_lines =
  pragma: no cover

    def __str__

omit=
    *__init__*
    *forms*
    *middleware*
    */migrations/*
    *manage*
    *serializers*
    *admin*
    *test*
    *urls*
    *settings*
    *wsgi*
    *apps*
